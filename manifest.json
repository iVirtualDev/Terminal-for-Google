{
	"name": "Terminal for Google",
	"version": "1.8.21",
	"description": "Quick access to Google!",
	
	"icons": {
		"128": "images/icon-128.png"
	},
	
	"permissions": [
		"contextMenus",
		"tabs",
		"*://*/*"
	],
	
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"css": [],
			"js": ["scripts/content.js"]
		}
	],
	
	"background_page": "back.html",
	"options_page": "views/option/index.html",
	
	"browser_action": {
		"default_title": "Terminal for Google",
		"default_icon": "images/goog-logo-old.png",
		"popup": "views/popup/index.html"
	}
}
